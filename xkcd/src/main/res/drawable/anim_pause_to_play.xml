<animated-vector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt" android:drawable="@drawable/ic_pause" xmlns:tools="http://schemas.android.com/tools" tools:targetApi="lollipop">
    <target android:name="pause">
        <aapt:attr name="android:animation">
            <objectAnimator android:propertyName="pathData" android:duration="300" android:valueFrom="M 5 6 L 5 10 L 19 10 L 19 6 L 5 6 M 5 14 L 5 18 L 19 18 L 19 14 L 5 14" android:valueTo="M 8 5 L 8 12 L 19 12 L 19 12 L 8 5 M 8 12 L 8 19 L 19 12 L 19 12 L 8 12" android:valueType="pathType" android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <objectAnimator android:propertyName="rotation" android:duration="300" android:valueFrom="90" android:valueTo="0" android:valueType="floatType" android:interpolator="@android:interpolator/accelerate_decelerate"/>
        </aapt:attr>
    </target>
</animated-vector>
